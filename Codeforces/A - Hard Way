#include<bits/stdc++.h>
using namespace std;
 
#define YES cout << "YES" << endl
#define Yes cout << "Yes" << endl
#define yes cout << "yes" << endl
#define No cout << "No" << endl
#define NO cout << "NO" << endl
#define no cout << "no" << endl
#define _1 cout << -1 << endl;
 
#define ll long long
#define ull unsigned long long
#define pii pair<int, int>
#define pll pair<ll, ll>
#define vi vector<int>
#define vs vector<char>
#define vl vector<ll>
#define vii vector<pii>
#define vll vector<pll>
#define pb push_back
#define ff first
#define ss second
int main()
{
    int t;
    cin>>t;
    ll int a,b,c,d,e,f;
    while(t--)
    {
        cin>>a>>b>>c>>d>>e>>f;
        if(b==d&&b>f)
            cout<<abs(a-c);
        else if(f==d&&f>b)
            cout<<abs(c-e);
        else if(b==f&&b>d)
            cout<<abs(a-e);
        else
            cout<<"0";
        cout<<endl;
    }
 
}